{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended", "next", "next/core-web-vitals", "prettier"],
  "rules": {
    "strict": "error",
    "no-unused-vars": ["error", { "args": "none" }],
    "no-console": "error",
    "no-inline-comments": "error",
    "no-var": "error",
    "no-duplicate-imports": "error"
  }
}
